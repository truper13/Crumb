        <item name="android:singleLine">true</item>
        <item name="android:selectAllOnFocus">true</item>
        <item name="android:editable">true</item>
        <item name="android:inputType">textNoSuggestions</item>
        <item name="android:imeOptions">actionGo|flagNoExtractUi</item>
    </style>
    <style name="gallery_output">
        <item name="android:paddingTop">50.0dip</item>
        <item name="android:paddingBottom">200.0dip</item>
        <item name="android:clipToPadding">false</item>
