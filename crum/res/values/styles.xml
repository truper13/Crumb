        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/omnibar_view_bg</item>
        <item name="android:focusable">false</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginTop">5.0dip</item>
        <item name="android:layout_marginBottom">5.0dip</item>
        <item name="android:shadowColor">@color/Blue</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="omnisearch_field">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">@color/White</item>
        <item name="android:textColorHint">@color/OmniSearchHint</item>
        <item name="android:ellipsize">end</item>
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
