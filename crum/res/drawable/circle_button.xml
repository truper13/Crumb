<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item android:state_pressed="true">
        <shape android:shape="1">
            <stroke android:width="1dp" android:color="@color/HoloBlue" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="1">
            <stroke android:width="1dp" android:color="@color/GoodGray" />
        </shape>
    </item>
</selector>
